# ACD Application Qualifiers
# Qualifier       Type        Default     Helptext
QualAppl {
  auto            boolean     "N"         "Turn off prompts"
  stdout          boolean     "N"         "Write first file to standard output"
  filter          boolean     "N"         "Read first file from standard input, write first file to standard output"
  options         boolean     "N"         "Prompt for standard and additional values"
  debug           boolean     "N"         "Write debug output to program.dbg"
  verbose         boolean     "N"         "Report some/full command line options"
  help            boolean     "N"         "Report command line options and exit. More information on associated and general qualifiers can be found with -help -verbose"
  warning         boolean     "Y"         "Report warnings"
  error           boolean     "Y"         "Report errors"
  fatal           boolean     "Y"         "Report fatal errors"
  die             boolean     "Y"         "Report dying program messages"
  version         boolean     "N"         "Report version number and exit"
}


